{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"types": ["vinxi/client"],
		"isolatedModules": true
	}
}

